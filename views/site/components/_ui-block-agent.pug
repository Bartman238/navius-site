mixin block-agent(obj)
  // Block Agent
  //- a.block-agent(href=obj.link ? obj.link : 'agent-single-page.html')
  span.block-agent
    //- +jpg(obj.img)
    iframe(width="270" height="230" src=`https://www.youtube.com/embed/${obj.youtubeId}` frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen)
    .block-agent-body
      h3.block-agent-title=obj.title
      p=obj.text
